{
    "name": "flexsim/flexnet-operations",
    "description": "A package to facilitate interaction with the Flexera FlexNet Operations soap services",
    "license": "MIT",
    "authors": [
        {
            "name": "cole.shirley",
            "email": "cole.shirley@flexsim.com"
        }
    ],
    "require": {
        "ext-soap": "^8.0",
        "phpro/soap-client": "^1.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.14",
        "laminas/laminas-code": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
            "Flexsim\\FlexnetOperations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flexsim\\FlexnetOperations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "testbench": "@php vendor/bin/testbench"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flexsim\\FlexnetOperations\\FlexnetOperationsServiceProvider"
            ],
            "aliases": {
                "Flexnet": "Flexsim\\FlexnetOperations\\Facades\\Flexnet"
            }
        }
    }
}