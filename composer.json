{
    "name": "flexsim/flexnet-operations",
    "description": "A package to facilitate interaction with the Flexera FlexNet Operations soap services",
    "license": "MIT",
    "authors": [
        {
            "name": "cole.shirley",
            "email": "cole.shirley@flexsim.com"
        }
    ],
    "require": {
        "ext-soap": "^8.0",
        "phpro/soap-client": "^1.4",
        "psr/http-message": "^1.0",
        "php-http/httplug": "^2.1",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.7",
        "php-http/message": "^1.8",
        "php-http/client-common": "^2.1",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.14",
        "laminas/laminas-code": "^3.1.0",
        "php-vcr/php-vcr": "^1.5",
        "php-vcr/phpunit-testlistener-vcr": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Flexsim\\FlexnetOperations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flexsim\\FlexnetOperations\\Assembler\\": "Assembler",
            "Flexsim\\FlexnetOperations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "testbench": "@php vendor/bin/testbench"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flexsim\\FlexnetOperations\\FlexnetOperationsServiceProvider"
            ],
            "aliases": {
                "Flexnet": "Flexsim\\FlexnetOperations\\Facades\\Flexnet"
            }
        }
    }
}
